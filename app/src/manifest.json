{
  "name": "Redirector App",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "app": {
    "background": {
      "scripts": ["bundles/background.bundle.js"]
    }
  },
  "web_accessible_resources": ["styles/livereload.js", "styles/bootstrap.min.js","styles/jquery.min.js", "styles/bootstrap.min.css", "styles/main.css"],
  "permissions": [
  "management",
    {"socket":["tcp-connect", "tcp-listen"]},
            "\u003Call_urls\u003E",
        "file://*",
        "file:///*",
        "chrome-extension://*",{"fileSystem": ["retainEntries", "directory"]}, "background",
        "storage"
  ]
}
